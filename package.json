{
  "name": "my-blog-site",
  "version": "1.0.0",
  "description": "This repository is a template on how to setup node express and typescript for projects. This will enable you integrate TypeScript into your nodejs project.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/src/index.js",
    "server": "nodemon dist/src/index.js",
    "server:ts": "SET NODE_ENV=DEVELOPMENT& npx nodemon backend/src/index.ts",
    "predev": "npm run build",
    "dev": "SET NODE_ENV=DEVELOPMENT& concurrently \"npx tsc -w\" \"npm run server\"",
    "prod": "SET NODE_ENV=PRODUCTION& concurrently \"npx tsc -w\" \"npm run server\"",
    "lint": "npx eslint .",
    "format": "prettier --write .",
    "lint:fix": "npx eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uimarshall/nodejs-express-typescript-template.git"
  },
  "keywords": [],
  "author": "UI Marshall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uimarshall/nodejs-express-typescript-template/issues"
  },
  "homepage": "https://github.com/uimarshall/nodejs-express-typescript-template#readme",
  "dependencies": {
    "@types/morgan": "^1.9.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "nanoid": "^3.0.0",
    "nodemailer": "^6.9.3",
    "rimraf": "^4.4.1",
    "slugify": "^1.6.6",
    "string-strip-html": "^13.4.1",
    "validator": "^13.9.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.19",
    "@types/nodemailer": "^6.4.8",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
